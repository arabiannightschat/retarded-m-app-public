  url('//at.alicdn.com/t/font_1710408_jcm2hp79rc.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1710408_jcm2hp79rc.woff') format('woff'),
  url('//at.alicdn.com/t/font_1710408_jcm2hp79rc.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1710408_jcm2hp79rc.svg#iconfont') format('svg');
}

